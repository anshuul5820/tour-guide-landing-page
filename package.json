{
  "name": "natours-project",
  "version": "1.0.0",
  "description": "a practise website for a tourist guide company",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devServer": "live-server",
    "start": "npm-run-all devServer watch:sass --parallel",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schuylerdare96/tour-guide-landing-page.git"
  },
  "author": "ansul",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schuylerdare96/tour-guide-landing-page/issues"
  },
  "homepage": "https://github.com/schuylerdare96/tour-guide-landing-page#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.7",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}